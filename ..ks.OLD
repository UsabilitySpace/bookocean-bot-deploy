# ks = killall + start

# main job

killall -9 node; clear; npm start

# ─────────────────────────────────

# value added in all public files

if [ -z "$DONT_PRE" ] ; then

  export DONT_PRE="1"

  SCRIPT_DIR="$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )"
  source "$SCRIPT_DIR/engine_redeploy_scripts"

  cls

fi

